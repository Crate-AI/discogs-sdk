{
  "name": "@crate.ai/discogs-sdk",
  "version": "1.1.2",
  "description": "an SDK for the Discogs API",
  "repository": {
    "type": "git",
    "url": "https://github.com/Crate-AI/discogs-sdk"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && microbundle --tsconfig.json --no-sourcemap",
    "dev": "microbundle watch --tsconfig.json --no-sourcemap"
  },
  "keywords": [
    "discogs",
    "sdk",
    "api",
    "oauth"
  ],
  "author": "paprikaf",
  "license": "ISC",
  "files": [
    "dist"
  ],
  "microbundle": {
    "globals": {
      "readline": "readline"
    },
    "external": ["readline"]
  },
  "devDependencies": {
    "microbundle": "^0.15.1"
  },
  "dependencies": {
    "isomorphic-unfetch": "^4.0.2"
  }
}
